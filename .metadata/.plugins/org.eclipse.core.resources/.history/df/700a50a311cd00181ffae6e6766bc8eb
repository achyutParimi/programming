package com.java.practise;

public class TestStack {
	int arr[];
	int top;
	TestStack(int n){
		arr = new int[5];
		top = -1;
		
	}
	boolean push(int a){
		if(top == arr.length) return false;
		else{
			top++;
			arr[top] = a;
			return true;
		}
		
	}
	int pop(){
		int num = 0;
		if(top == 0) return -1;
		else {
			num = arr[top];
			top--;
		}
		return num;
	}
	public static void main(String []args){
		int size = 10;
		TestStack ts = new TestStack(size);
		if(ts.push(5)){
			System.out.println("the element added is"+ts.pop());
		}
	}

}
